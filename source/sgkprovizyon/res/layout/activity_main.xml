		                    style="@style/edt_result_style">
		                </EditText>
		            </LinearLayout>
		            
		            
		            <LinearLayout
		                android:id="@+id/lline11"
		                android:layout_width="match_parent"
		                android:layout_height="wrap_content"
		                android:background="@drawable/bg_gradient_control"
		                android:padding="4dip" >
		                <TextView
		                    android:id="@+id/lblSigortaliTuru"
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:shadowColor="#FFFFFF"
		                    android:shadowDy="1"
		                    android:shadowRadius="2"
		                    style="@style/txt_style"
		                    android:text="Sigoratalı Türü"
		                    android:textSize="15sp"
