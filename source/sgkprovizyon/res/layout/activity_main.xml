		                    android:layout_width="wrap_content"
		                    android:layout_height="match_parent"
		                    android:shadowColor="#FFFFFF"
		                    android:shadowDy="1"
		                    android:shadowRadius="2"
		                    android:text="Katılım Payı Muafiyeti"
		                    style="@style/txt_style"
		                    android:textSize="15sp"
		                    android:width="130dp" />
		                <EditText
		                    android:id="@+id/edtKatiliPayiMuafiyeti"
		                    android:layout_width="match_parent"
		                    android:layout_height="wrap_content"
		                    android:layout_weight="1"
		                    android:ems="10"
		                    android:textSize="15sp" 
		                    style="@style/edt_result_style">
		                </EditText>
		            </LinearLayout>
		            
		           
		           
		           <LinearLayout
		                android:id="@+id/lline7"
		                android:layout_width="match_parent"
		                android:layout_height="wrap_content"
		                android:background="@drawable/bg_gradient_control"
		                android:padding="4dip" >
		                <TextView
		                    android:id="@+id/lblSgkKapsami"
		                    android:layout_width="wrap_content"
