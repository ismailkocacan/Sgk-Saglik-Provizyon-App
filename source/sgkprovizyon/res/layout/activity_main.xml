            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
                
		            <LinearLayout
		                android:id="@+id/lline32"
		                android:layout_width="match_parent"
		                android:layout_height="wrap_content"
		                android:background="@drawable/bg_gradient_control"
		                android:padding="4dip" >
		                <TextView
		                    android:id="@+id/lblAdiSoyadi"
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:shadowColor="#FFFFFF"
		                    android:shadowDy="1"
		                    android:shadowRadius="2"
		                    android:text="Adı Soyadı             "
		                    android:textSize="15sp"
		                    style="@style/txt_style"
		                    android:width="130dp" />
		                <EditText
		                    android:id="@+id/edtAdiSoyadi"
		                    android:layout_width="match_parent"
		                    android:layout_height="wrap_content"
		                    android:layout_weight="1"
		                    android:ems="10"
		                    android:textSize="15sp" 
		                    style="@style/edt_result_style">
		                </EditText>
		            </LinearLayout>
		            
		            
		            
		           <LinearLayout
		                android:id="@+id/lline33"
		                android:layout_width="match_parent"
