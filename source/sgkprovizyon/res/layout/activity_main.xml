		                <EditText
		                    android:id="@+id/edtSigorataliTuru"
		                    android:layout_width="match_parent"
		                    android:layout_height="wrap_content"
		                    android:layout_weight="1"
		                    android:ems="10"
		                    android:textSize="15sp" 
		                    style="@style/edt_result_style">
		                </EditText>
		            </LinearLayout>
		            
		            
		          <LinearLayout
		                android:id="@+id/lline12"
		                android:layout_width="match_parent"
		                android:layout_height="wrap_content"
