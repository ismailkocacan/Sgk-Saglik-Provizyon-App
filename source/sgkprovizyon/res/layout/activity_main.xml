		                android:padding="4dip" >
		                <TextView
		                    android:id="@+id/lblDogumTarihi"
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:shadowColor="#FFFFFF"
		                    android:shadowDy="1"
		                    android:shadowRadius="2"
		                    android:text="Doğum Tarihi             "
