		                    android:layout_height="wrap_content"
		                    android:layout_weight="1"
		                    android:ems="10"
		                    android:textSize="15sp" 
		                    style="@style/edt_result_style">
		                </EditText>
		            </LinearLayout>
		            
		            
		            
		            <LinearLayout
		                android:id="@+id/lline10"
		                android:layout_width="match_parent"
		                android:layout_height="wrap_content"
		                android:background="@drawable/bg_gradient_control"
		                android:padding="4dip" >
		                <TextView
		                    android:id="@+id/lblYaralandigiKisi"
		                    android:layout_width="wrap_content"
		                    android:layout_height="wrap_content"
		                    android:shadowColor="#FFFFFF"
		                    android:shadowDy="1"
		                    android:shadowRadius="2"
		                    style="@style/txt_style"
		                    android:text="Yararlandiği Kişi"
		                    android:textSize="15sp"
		                    android:width="130dp" />
